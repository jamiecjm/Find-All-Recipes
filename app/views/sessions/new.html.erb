<div class="sign-in">


<%= form_tag sessions_path do %>
  <div class="form-group field">
    <%= email_field_tag :email,nil, autofocus: true, class: "form-control", placeholder: "Email" %>
  </div>

  <div class="form-group field">
    <%= password_field_tag :password,nil, autocomplete: "off", class: "form-control", placeholder: "Password"  %>
  </div>



    <%= submit_tag "Log in", class: "btn btn-default" %>


<% end %>


</div>